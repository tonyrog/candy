#!/usr/bin/env xdg-open

[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Icon[sv_SE]=gnome-panel-launcher
Name[sv_SE]=Candy
Exec=erl -noshell -env ERL_LIBS $HOME/erlang -s candy -config $HOME/candy/sys.config
Comment[sv_SE]=CAN bus analyser
Name=Candy
Comment=CAN bus analyser
Icon=$HOME/erlang/priv/candy.png
